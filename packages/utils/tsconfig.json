{
  "extends": "@repo/typescript-config/base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ES5",
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules"
  ]
}