{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env",
    ".env.local",
    "tsconfig.json"
  ],
  "globalDotEnv": [
    ".env",
    ".env.local"
  ],
  "pipeline": {
    "build": {
      "env": [
        "GOOGLE_CLIENT_ID",
        "GOOGLE_CLIENT_SECRET"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "serve": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}